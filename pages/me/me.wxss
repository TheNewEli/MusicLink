page{
  height: 100%;
  width: 100%;
}
.me-container{
  display: flex;
  flex-direction: column;
  align-items: center;
}

.me-userInfo{
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 750rpx;
  height: 350rpx;
  background-color: #F5F5F5;
}

.me-userInfo image{
  margin-top: 5%;
  width: 200rpx;
  height: 200rpx;
  border-radius: 50%;
}

.me-userInfo text{
  margin-top: 10px;
  font-size: 18px;
  font-family: Microsoft YaHei;
}

.me-settings{
  display: flex;
  flex-direction: column;
  align-items: center;
}

.me-menuitems{
  display: flex;
  flex-direction: row;
  align-items: center;
  padding: 13px 30px 13px 40px;
  border-bottom: 2px solid #F5F5F5;
  width: 750rpx;
}

.me-menuitems-interval{
  display: flex;
  flex-direction: row;
  align-items: center;
  padding: 13px 30px 13px 40px;
  border-top: 10px solid #F5F5F5;
  border-bottom: 2px solid #F5F5F5;
  width: 750rpx;
}

.menuitems-text{
  font-size: 16px;
  font-family: Microsoft YaHei;
  color: #696969;
  width: 670rpx;
}

.menuitems-image{
  height: 16px;
  width: 16px;
  align-items: right;
}